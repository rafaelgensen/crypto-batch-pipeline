{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3ServerAccessLogsPolicy",
      "Effect": "Allow",
      "Principal": {
        "Service": "logging.s3.amazonaws.com"
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::logs-coingecko-staging-663354324751/logs/*",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "663354324751"
        }
      }
    }
  ]
}
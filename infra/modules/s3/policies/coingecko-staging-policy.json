{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListBucket",
      "Effect": "Allow",
      "Principal": {
        "AWS": [
          "arn:aws:iam::663354324751:role/glue-ingest-role",
          "arn:aws:iam::663354324751:role/glue-bronze-role"
        ]
      },
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::coingecko-staging-663354324751",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "663354324751"
        }
      }
    },
    {
      "Sid": "AllowObjectActions",
      "Effect": "Allow",
      "Principal": {
        "AWS": [
          "arn:aws:iam::663354324751:role/glue-ingest-role",
          "arn:aws:iam::663354324751:role/glue-bronze-role"
        ]
      },
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::coingecko-staging-663354324751/*",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "663354324751"
        }
      }
    }
  ]
}
